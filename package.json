{
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "cspell": "^6.12.0",
    "jest": "^29.1.2",
    "jest-junit": "^14.0.1",
    "rimraff": "^0.0.1-security",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "rome": "^11.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf node_modules reports coverage tsconfig.tsbuildinfo",
    "lint:fix": "npx rome format . --write",
    "lint": "npx rome check . && cspell \"**\" --dot --exclude .git --gitignore",
    "test": "jest --config ./jest.config.ts",
    "validate": "npm run build && npm run lint:fix && npm run lint && npm test"
  }
}
